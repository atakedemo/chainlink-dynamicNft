{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "baseUrl": "src"
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
}